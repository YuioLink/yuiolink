        <div class="row" style="margin-bottom:20px">
            <div class="col-md-8 col-md-offset-2">
                {% block outer_content %}
                {% block content %}{% endblock %}
                {% endblock %}
            </div>
        </div>
    </div>

    {% block script %}{% endblock %}
</body>
</html>
